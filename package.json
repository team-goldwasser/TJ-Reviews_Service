{
  "name": "rotten_tomatoes_reviews_service",
  "version": "1.0.0",
  "description": "Rotten Tomatoes User Reviews",
  "main": "app.js",
  "scripts": {
    "react": "webpack -d --watch",
    "start-dev": "NODE_ENV=development node server/app.js",
    "start-prod": "NODE_ENV=production node server/app.js",
    "test": "export NODE_ENV=UAT && mysql -u root -p <test/db/UATschema.sql && jest",
    "seed": "mysql -u root -p <server/schema.sql && node data/scripts/generateMovieReviews.js && node data/scripts/generateUsers.js && node server/database/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/number-ape/Rotten_Tomatoes_Reviews_Service.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/number-ape/Rotten_Tomatoes_Reviews_Service/issues"
  },
  "homepage": "https://github.com/number-ape/Rotten_Tomatoes_Reviews_Service#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "axios": "0.18.0",
    "babel-loader": "^8.0.5",
    "babel-preset-jest": "^24.1.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "fast-csv": "^2.4.1",
    "jest": "^24.1.0",
    "jquery": "^3.4.0",
    "supertest": "3.4.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "eslint": "^5.13.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "nodemon": "^1.18.10",
    "pg": "7.10.0",
    "react": "^16.8.2",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.8.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>client/src/tests/setupTest.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
